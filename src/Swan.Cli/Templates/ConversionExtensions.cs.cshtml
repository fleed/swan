@model Swan.Generators.Models.ProjectViewModel
namespace @this.Model.Namespace
{
    using System.Collections.Generic;
    using System.Linq;
    
    using Entities;

    @foreach(var entity in this.Model.Entities)
    { 
    @:using Dto@(entity.Name) = Dto.@entity.Name;
    }

    public static class ConvertionExtensions
    {
        @foreach (var entity in this.Model.Entities)
        {
            await Html.RenderPartialAsync("ConversionExtensions.Convert.cs", entity);
        }

    }
}